{
    "name": "mortezamasumi/fb-essentials",
    "description": "This is my package fb-essentials",
    "keywords": [
        "mortezamasumi",
        "laravel",
        "fb-essentials"
    ],
    "homepage": "https://github.com/mortezamasumi/fb-essentials",
    "support": {
        "issues": "https://github.com/mortezamasumi/fb-essentials/issues",
        "source": "https://github.com/mortezamasumi/fb-essentials"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Morteza Masumi",
            "email": "morteza.masumi@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "ariaieboy/filament-jalali": "^2.0",
        "bezhansalleh/filament-language-switch": "^4.0",
        "bezhansalleh/filament-shield": "^4.0",
        "filament/filament": "^4.0",
        "filament/spatie-laravel-google-fonts-plugin": "^4.0",
        "lara-zeus/spatie-translatable": "^1.0",
        "spatie/laravel-package-tools": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-browser": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "pestphp/pest-plugin-livewire": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Mortezamasumi\\FbEssentials\\": "src/",
            "Mortezamasumi\\FbEssentials\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mortezamasumi\\FbEssentials\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mortezamasumi\\FbEssentials\\FbEssentialsServiceProvider"
            ],
            "aliases": {
                "FbEssentials": "Mortezamasumi\\FbEssentials\\Facades\\FbEssentials"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
